{
    "name": "bolt/disqus",
    "description": "The 'Disqus' extension inserts a Disqus comment thread in your templates.",
    "type": "bolt-extension",
    "keywords": ["disqus", "comments"],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bolt\\Disqus\\": "src"
        }
    },
    "extra": {
        "bolt-icon": "web/disqus.png",
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Bolt\\Disqus\\DisqusExtension"
    }
}
